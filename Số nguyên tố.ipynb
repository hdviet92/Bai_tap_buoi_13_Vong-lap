    "# số nguyên tố\n",
    "\n",
    "n = int(input('Nhập một số tự nhiên: '))\n",
    "nt = True\n",
    "for i in range (2, n//2):\n",
    "    if n%i == 0:\n",
    "        nt = False\n",
    "        break\n",
    "if nt:\n",
    "    print('yes')\n",
    "else:\n",
    "    print('no')\n",
    "    "
